  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "pages:build": "opennextjs-cloudflare",
    "pages:deploy": "wrangler pages deploy",
    "pages:dev": "wrangler pages dev .open-next/worker --compatibility-flag=nodejs_compat",
    "cf:d1:create": "wrangler d1 create bjjh-cleaning-db",
    "cf:d1:migrate": "wrangler d1 migrations apply bjjh-cleaning-db --remote",
