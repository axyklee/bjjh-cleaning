# .env.example

# Secret used for authentication (set in production)
AUTH_SECRET=

# Trust host header (true/false)
AUTH_TRUST_HOST=

# Google OAuth credentials
AUTH_GOOGLE_CLIENT_ID=
AUTH_GOOGLE_CLIENT_SECRET=

# Database connection URL
DATABASE_URL=

# MinIO configuration
MINIO_ENDPOINT=
MINIO_PORT=
MINIO_ACCESS_KEY=
MINIO_SECRET_KEY=
MINIO_BUCKET=
MINIO_USE_SSL=

# Allowed emails (JSON array, e.g. ["user@example.com"])
ALLOWED_EMAILS=[]

# Node environment: development, test, or production
NODE_ENV=